<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config version="1.0">
    <coverageName name="raster"/>
    <coordsys name="EPSG:3067"/>
    <!-- interpolation 1 = nearest neighbour, 2 = bipolar, 3 = bicubic -->
    <scaleop interpolation="1"/>
    <axisOrder ignore="false"/>
    <spatialExtension name="pgraster"/>
    <mapping>
        <masterTable name="raster_metadata">
            <coverageNameAttribute name="coverage"/>
            <maxXAttribute name="max_x"/>
            <maxYAttribute name="max_y"/>
            <minXAttribute name="min_x"/>
            <minYAttribute name="min_y"/>
            <resXAttribute name="res_x"/>
            <resYAttribute name="res_y"/>
            <tileTableNameAtribute name="tile_table"/>
        </masterTable>
        <tileTable>
            <blobAttributeName name="rast"/>
        </tileTable>
    </mapping>
    <connect>
        <dstype value="DBCP"/>
        <username value="nlsmap"/>
        <password value="nlsmap"/>
        <jdbcUrl value="jdbc:postgresql://localhost/nlsmap"/>
        <driverClassName value="org.postgresql.Driver"/>
        <maxActive value="10"/>
        <maxIdle value="0"/>
    </connect>
</config>
